{
  "name": "mphpmaster/laravel-scroll-to-top",
  "version": "1.0.0",
  "description": "Laravel Scroll page to top package.",
  "type": "library",
  "keywords": [
    "Laravel",
    "Scroll",
    "Scroll page to top"
  ],
  "require": {
    "php": ">=8.1",
    "laravel/framework": ">=9",
    "illuminate/support": ">=9",
    "laravel/helpers": "^1.5"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "mPhpMaster",
      "email": "59211285+mPhpMaster@users.noreply.github.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MPhpMaster\\LaravelScrollToTop\\": "src/"
    },
    "files": [
      "src/Helpers/FHelpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MPhpMaster\\LaravelScrollToTop\\Providers\\HelperProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "scripts": {
    "c:c": [
      "@php artisan cache:clear",
      "@php artisan route:clear",
      "@php artisan config:clear",
      "@php artisan view:clear"
    ],
    "a:routes": [
      "@php artisan route:list --no-ansi > routes.txt"
    ],
    "m:c": [
      "mkdir -p storage/framework/{sessions,views,cache}",
      "chmod -R 775 storage/framework",
      "chown -R `whoami`:nobody storage/framework"
    ]
  }
}
